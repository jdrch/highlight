{
    "comments": false,
    "presets": [
        [
            "@babel/env", {
                "modules": false,

                "targets": {
                    "esmodules": true
                }
            }
        ],
        ["minify", {
            "removeConsole": true
        }]
    ],
    "plugins": [
        "babel-plugin-remove-template-literals-whitespace"
    ]
}